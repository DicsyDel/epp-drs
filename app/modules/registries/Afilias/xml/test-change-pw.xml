<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<epp xmlns="urn:ietf:params:xml:ns:epp-1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:ietf:params:xml:ns:epp-1.0 epp-1.0.xsd">
<command>
	<login>
		<clID>{clid}</clID>
		<pw>{pw}</pw>
		<newPW>{newPw}</newPW>		
		<options>
			<version>1.0</version>
			<lang>{lang}</lang>
		</options>
		<svcs>
			<objURI>urn:ietf:params:xml:ns:domain-1.0</objURI>
			<objURI>urn:ietf:params:xml:ns:host-1.0</objURI>
			<objURI>urn:ietf:params:xml:ns:contact-1.0</objURI>
			<svcExtension>
				<extURI>urn:afilias:params:xml:ns:rrpproxy-1.0</extURI>
				<extURI>urn:afilias:params:xml:ns:oxrs-1.0</extURI>
				<extURI>urn:EPP:xml:ns:ext:rgp-1.0</extURI>
				<extURI>urn:afilias:params:xml:ns:idn-1.0</extURI>
				<extURI>urn:afilias:params:xml:ns:bulk-1.0</extURI>
				<extURI>urn:afilias:params:xml:ns:mobi-1.0</extURI>
				<extURI>urn:afilias:params:xml:ns:ext:info-trademark-1.0</extURI>
			</svcExtension>
		</svcs>
	</login>
	<clTRID>{clTRID}</clTRID>
</command>
</epp>